<body>
	<button type="button" class="btn btn-outline-success" id="btn">food데이터
		가져오기</button>
	<br>
	<br>
	<div id="result">결과</div>

	<script type="text/javascript">
		$("#btn1").click(function() {

			$.ajax({
				type : "get",
				url : "data3.xml",
				dataType : "xml",
				success : function(data) {

					var s = "";
					//idx는 0부터,ele는 각태그값,여기서는 stu각각이 ele로 넘어옴
					$(data).find("stu").each(function(idx, ele) {
						var e = $(ele);

						s += "idx:" + idx + "<br>";
						s += "이름: " + e.attr("name") + "<br>";
						s += "Java: " + e.find("java").text() + "<br>";
						s += "Spring: " + e.find("spring").text() + "<br>";
						s += "Ajax: " + e.find("ajax").text() + "<br>";
						s += "--------------------<br>";
					});

					$("#result").html(s);
				}
			})
		});

	